generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// Criando a tabela no formato do Prisma
model Feedback {
  id String @id @default(uuid()) // Universal Unique ID

  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}
